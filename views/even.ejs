<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Product</title>
    <link rel="stylesheet" href="css/even.css">
    <link rel="stylesheet" href="font/css/all.css">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
</head>
<body>
<%- include('head.ejs'); %>
<main>
    <div id="showcase">
        <div id="container">
            <div id="content">
                <p>shop</p>
            </div>
        </div>
    </div>
    <div id="products">
        <% products.map(function (e) { %>
        <div class="card">
            <div class="list-product">
                <div class="img-block">
                    <a href="#">
                        <img class="img-product" src="<%= e.photo %>" alt="">
                    </a>
                </div>
                <div class="detail">
                    <a class="butomproducts" href="/products-detail/<%= e.id %>">CHI TIáº¾T</a>
                </div>
                <div class="product-decs">
                    <h2><a href="#"><%= e.name %></a></h2>
                    <div class="pricing-meta">
                        <p><%= e.price%></p>
                    </div>
                </div>
            </div>
        </div>
        <% })%>
    </div>
</main>
<footer>
    <a href="https://www.facebook.com/dongcave97" target="_blank"><div class="social-icon"><i class="fab fa-facebook-f"></i></div></a>
    <a href="#"><div class="social-icon" ><i class="fab fa-instagram"></i></div></a>
    <a href="#"><div class="social-icon"><i class="fab fa-youtube"></i></div></a>
</footer>
</body>
</html>